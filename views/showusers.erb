<table class="table">

	<tr>
		<td>Username</td>
		<td>Phone</td>
		<td>Datestamp</td>
		<td>Barbername</td>
		<td>Color</td>
	</tr>
	
	<% @results.each do |row| %>

		<tr>
			<td><%= row['username'] %></td>
			<td><%= row['phone'] %></td>
			<td><%= row['datestamp'] %></td>
			<td><%= row['barbername'] %></td>
			<td><%= row['color'] %></td>
		</tr>

	<% end %>

</table>